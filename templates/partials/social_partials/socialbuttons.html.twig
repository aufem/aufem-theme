<div class="center">
<ul class="rrssb-buttons">
    {% set page_url = socialbuttons_domain ~ url %}
    {% for name, values in socialbuttons_buttons %}

    {% if values.enabled %}
    	
        {% include 'partials/social_partials/_' ~ name ~ '.html.twig' with {'label': values.label} %}
        
    {% endif %}
    {% endfor %}
</ul>
</div>